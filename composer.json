{
  "name": "wds/sisense-backend",
  "description": "Plugins and theme to power the website.",
  "type": "project",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "WebDevStudios",
      "email": "contact@webdevstudios.com"
    }
  ],
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://packages.wdslab.com"
    }
  ],
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  },
  "require": {
    "webdevstudios/advanced-custom-fields-pro": "5.11.4",
    "wpackagist-plugin/aryo-activity-log": "2.8.1",
    "wpackagist-plugin/add-wpgraphql-redirection": "0.0.3",
    "wpackagist-plugin/add-wpgraphql-seo": "4.16.0",
    "wpackagist-plugin/advanced-admin-search": "1.1.2",
    "wpackagist-plugin/tinymce-advanced": "5.6.0",
    "wpackagist-plugin/akismet": "4.2.1",
    "wpackagist-plugin/amazon-web-services": "1.0.5",
    "wpackagist-plugin/atlas-content-modeler": "0.12.1",
    "wpackagist-plugin/custom-post-type-ui": "1.10.1",
    "wpackagist-plugin/ewww-image-optimizer": "6.3.0",
    "wpackagist-plugin/genesis-blocks": "1.3.0",
    "wpackagist-plugin/import-users": "1.2.9",
    "wpackagist-plugin/import-woocommerce": "1.6.7",
    "wpackagist-plugin/better-wp-security": "8.0.2",
    "wpackagist-plugin/post-type-switcher": "3.2.1",
    "wpackagist-plugin/onelogin-saml-sso": "3.4.0",
    "wpackagist-plugin/redirection": "5.1.3",
    "wpackagist-plugin/google-site-kit": "1.48.1",
    "wpackagist-plugin/svg-support": "2.3.20",
    "wpackagist-plugin/wordpress-importer": "0.7",
    "wpackagist-plugin/wp-graphql": "1.6.10",
    "deliciousbrains-plugin/wp-migrate-db-pro": "2.2.2",
    "deliciousbrains-plugin/wp-migrate-db-pro-media-files": "2.1.0",
    "wpackagist-plugin/wp-ultimate-csv-importer": "6.4.4",
    "wpackagist-plugin/wp-ultimate-exporter": "1.7.1",
    "wpackagist-plugin/wp-user-avatars": "1.4.1",
    "wpackagist-plugin/duplicate-post": "4.3",
    "wpackagist-plugin/wordpress-seo": "17.8"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
    "wp-coding-standards/wpcs": "^2.3",
    "phpcompatibility/phpcompatibility-wp": "^2.1"
  },
  "scripts": {
    "post-autoload-dump": "WebDevStudios\\MUAutoload\\Installer::install",
    "format": "phpcbf --report=summary,source",
    "lint": "phpcs --report=summary,source",
    "build": [
        "@composer install"
    ]
  },
  "config": {
    "platform": {
      "php": "7.4"
    }
  }
}
